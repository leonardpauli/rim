{
	"name": "@leonardpauli/rim",
	"version": "0.1.0",
	"description": "Declarative + reactive language, parser + WIP eval",
	"main": "index.js",
	"scripts": {
		"test:coverage": "jest --coverage",
		"test:watch": "jest --changedFilesWithAncestor --watch",
		"test": "jest",
		"lint": "eslint --fix '{src,__tests__}/**/*.js'",
		"lint:watch": "onchange '{src,__tests__}/**/*.js' -d 200 -w -- npm run --silent lint {{changed}}",
		"build:watch": "npm run build -- --watch",
		"build": "f () { ([ -d lib ] && rm -rf lib); babel src -d lib --copy-files --include-dotfiles --ignore '**/*.test.js' --source-maps ${1:---retain-lines --minified}; }; f"
	},
	"keywords": [],
	"author": "Leonard Pauli <leonardpauli@me.com> (https://leonardpauli.com)",
	"private": false,
	"license": "AGPL-3.0-or-later",
	"files": [
		"lib/",
		"index.js",
		"__tests__/"
	],
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta.52",
		"@babel/core": "^7.0.0-beta.52",
		"@babel/preset-env": "^7.0.0-beta.52",
		"@babel/preset-stage-2": "^7.0.0-beta.52",
		"babel-preset-minify": "^0.3.0",
		"@leonardpauli/babel-preset": "^0.1.0",
		"@leonardpauli/eslint-config": "^0.1.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"eslint": "^4.19.1",
		"eslint-plugin-jest": "^21.15.2",
		"eslint-plugin-vue": "^4.4.0",
		"jest": "^22.4.3",
		"lint-staged": "^7.0.4",
		"onchange": "^3.3.0",
		"yorkie": "^1.0.3"
	},
	"lint-staged": {
		"{src,examples}/**/*.js": [
			"npm run lint --"
		]
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"jest": {
		"verbose": true
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"dependencies": {}
}
